                                                        <ul class="mini-crat-list list-unstyled d-flex m-0">
                                                            {% assign variantoptioncounter=0 %}
                                                            {% for key in
                                                            line_item.variant.metafields.productvariantoption %}
                                                            {% if key[1]!=blank %}
                                                            <li>{{ key[1] }}</li>
                                                            {% assign variantoptioncounter=1 %}
                                                            {% if forloop.index!=forloop.length %}
                                                            <li>|</li>
                                                            {% endif %}
                                                            {% endif %}
                                                            {% endfor %}
                                                            {% if variantoptioncounter>0 %}
                                                            <span><i class="fa fa-pencil" aria-hidden="true"></i></span>
                                                            {% endif %}
                                                        </ul>
                                                    </div>
                                                    <div class="qunatity-and-price d-flex justify-content-between">
                                                        <div class="qunatity">
                                                            QTY <span>{{ line_item.quantity }}</span>
                                                        </div>
                                                        <div class="mini-crat-price">
                                                            <p> {{ line_item.final_line_price
                                                                |money_without_trailing_zeros }}</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <hr>
                                            {% endfor %}
                                        </div>

                                        <div class="mini-cart-button d-flex justify-content-between pt-4">
                                            <button class="view-crat-button-minicart"
                                                onclick="location.href='/cart'">View Cart <span>({{ cart.item_count
                                                    }})</span> </button>
                                            <button class="checkout-button-minicart"
                                                onclick="location.href='/checkout'">Checkout
                                                <span>({{ cart.total_price | money_without_trailing_zeros
                                                    }})</span></button>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </div>

    <div class="search-bar-modal">
        <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
        <div class="modal fade" id="search" tabindex="-1" aria-hidden="true" aria-labelledby="searchlabel"
            aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <predictive-search>
                        <form action="{{ routes.search_url }}" method="get" role="search">
                            <div class="modal-body">
                                <div class="search-bar d-flex p-3">

                                    <button type="submit"><img
                                            src="https://cdn.shopify.com/s/files/1/0623/4567/4925/files/Search_Icon_c74288dc-060e-4db5-9111-4bfc0ee5b079.svg?v=1664444120"
                                            alt="Bell" class="img-fluid"></button>
                                    <input type="text" placeholder="|Seek, and you shall find" id="searchbox" name="q"
                                        class="w-100" role="combobox" aria-expanded="false"
                                        aria-owns="predictive-search-results-list"
                                        aria-controls="predictive-search-results-list" aria-haspopup="listbox"
                                        aria-autocomplete="list" autocorrect="off" autocomplete="off"
                                        autocapitalize="off" spellcheck="false"></input>
                                    <button type="button" class="search" data-bs-dismiss="modal"
                                        aria-label="Close">&#10005;</button>

                                </div>
                                <div class="search-bar-results mt-4">
                                    <div id="predictive-search" tabindex="-1"></div>
                                    <div id="recent-search" tabindex="-1"></div>
                                    <div class="search-trending-product py-2" id="top-searches">
                                        {%- render 'custom-trending-product' ,collectionhandle: 'top-searches' %}
                                    </div>
                                    <div class="search-trending-product py-2" id="trending-products">
                                        {%- render 'custom-trending-product', collectionhandle: 'trending-products' %}
                                    </div>

                                </div>

                            </div>
                        </form>
                    </predictive-search>
                </div>
            </div>
        </div>
    </div>
    <script>
        $('.dropdown-menu a.dropdown-toggle').on('mouseover', function (e) {
            if (!$(this).next().hasClass('show')) {
                $(this).parents('.dropdown-menu').first().find('.show').removeClass("show");
            }
            var $subMenu = $(this).next(".dropdown-menu");
            $subMenu.toggleClass('show');

            $(this).parents('li.nav-item.dropdown.show').on('mouseout', function (e) {
                $('.dropdown-submenu .show').removeClass("show");
            });
          
            return false;
          });

      
        $(document).ready(function () {
            $("#search").on('shown.bs.modal', function () {
                $(this).find('#searchbox').focus();
            });
        });
        function hideshow() {
            document.getElementById("attto").style.display = "block"
            setTimeout(function () { $("#attto").fadeOut(1500); }, 5000)
        }

        function showminicart() {
            document.getElementById("minicartpop").style.display = "block";
            document.getElementById("backroun-blk").classList.add("backroun-blk");
            document.body.style.overflow = "hidden";
        }

        function hideminicart() {
            document.getElementById("minicartpop").style.display = "none";
            document.getElementById("backroun-blk").classList.remove("backroun-blk");
            document.body.style.overflow = "inherit";
        }

        let rot = document.querySelector(".explore-store-toggle");
        rot.addEventListener("click", function () {
            document.getElementById("rotate").classList.toggle("rotateicon");
        })

    </script>
    <script>
        function myFunction() {
            document.body.classList.toggle("next")
            document.getElementById("mobileNavbar").classList.toggle("Hello")
            document.getElementById("scrollFixed").classList.toggle("mystyle")
        }

        var header = document.getElementById("navbarSupportedContent");
        var btns = header.getElementsByClassName("nav-item");
        for (var i = 0; i < btns.length; i++) {
            btns[i].addEventListener("click", function () {
                var current = document.getElementsByClassName("active");
                current[0].className = current[0].className.replace(" active", "");
                this.className += " active";
            });
        }

    </script>

    <script>
        $(".dropdown").hover(
            function () {
                $("#backroun-blk").addClass("backroun-blk");
            },
            function () {
                $("#backroun-blk").removeClass("backroun-blk");
            }
        );
    </script>
</header>


